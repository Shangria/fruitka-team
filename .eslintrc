{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "rules": {
        "max-len": [
            1,
            70,
            2,
            {
                "ignoreComments": true
            }
        ]
    }
}